    },
    {
      "name": "openstack/panko",
      "version_requirement": ">=10.3.1 <11.0.0"
    },
    {
      "name": "openstack/aodh",
      "version_requirement": ">=10.4.1 <11.0.0"
    },
    {
      "name": "puppet/zabbix",
      "version_requirement": ">=4.1.3 <5.0.0"
    }
  ]
