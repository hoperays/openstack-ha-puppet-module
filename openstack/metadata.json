{
  "name": "hoperays-openstack",
  "version": "0.2.0",
  "author": "hoperays",
  "summary": "Installs and configures OpenStack Platform.",
  "license": "Apache-2.0",
  "source": "https://github.com/hoperays/openstack-ha-puppet-module.git",
